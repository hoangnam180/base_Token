                              </div>\r\n                                 <div class=\"card-body table-full-width table-responsive\">\r\n                                     <table class=\"table table-hover table-striped\">\r\n                                         <thead>\r\n                                             <th>Mã Hóa Đơn</th>\r\n                                             <th>Tên Khách Hàng</th>\r\n                                         </thead>\r\n                                         <tbody>\r\n                                            <% data.forEach(bill => { %>\r\n                                                <tr>\r\n                                                    <td><%= bill.id %></td>\r\n                                                    <td> <% for (let i = 0; i < data.length; i++) { %>\r\n                                                    \r\n                                                    </td>\r\n                                                    <td><%= bill.name_product %></td>\r\n                                                    <td><%= bill.price %></td>\r\n                                                    <td style=\"display:flex;gap: 20px;justify-content: flex-end;\">\r\n                                                        <button data-toggle=\"modal\" data-target=\"#delete-bill\" data-id=\"<%= bill.id %>\"  type=\"button\" style=\"cursor: pointer;\" class=\"btn btn-danger\">Xóa</button>\r\n                                                </td>\r\n                                                </tr>\r\n                                                <% }) %>\r\n                                         </tbody>\r\n                                     </table>\r\n                             </div>\r\n                         </div>\r\n                     </div>\r\n                     <form name=\"delete-bills-form\" method=\"POST\"></form>\r\n                        <!-- Modal1 -->\r\n                        <div class=\"modal fade\" id=\"delete-bill\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n                            <div class=\"modal-dialog\" role=\"document\">\r\n                            <div class=\"modal-content\">\r\n                                <div class=\"modal-header\">\r\n                                <h5 class=\"modal-title\" id=\"exampleModalLabel\">Xóa hóa đơn</h5>\r\n                                <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n                                    <span aria-hidden=\"true\">&times;</span>\r\n                                </button>\r\n                                </div>\r\n                                <div class=\"modal-body\">\r\n                                Bạn có chắc muốn xóa hóa đơn này không\r\n                                </div>\r\n                                <div class=\"modal-footer\">\r\n                                    <button id=\"btn-delete-bill\" type=\"button\" class=\"btn btn-danger\">Xóa</button>\r\n                                    <button type=\"button\" class=\"btn btn-primary\" data-dismiss=\"modal\">Hủy bỏ</button>\r\n                                </div>\r\n                            </div>\r\n                            </div>\r\n                        </div>\r\n                 </div>\r\n             </div>\r\n             \r\n         </div>\r\n     </div>\r\n\r\n </body>\r\n <!--   Core JS Files   -->\r\n <script src=\"../assets/js/core/jquery.3.2.1.min.js\" type=\"text/javascript\"></script>\r\n <script src=\"../assets/js/core/popper.min.js\" type=\"text/javascript\"></script>\r\n <script src=\"../assets/js/core/bootstrap.min.js\" type=\"text/javascript\"></script>\r\n <!--  Plugin for Switches, full documentation here: http://www.jque.re/plugins/version3/bootstrap.switch/ -->\r\n <script src=\"../assets/js/plugins/bootstrap-switch.js\"></script>\r\n <!--  Google