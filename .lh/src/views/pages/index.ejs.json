            "name": "Commit-0",
            "content": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <%- include('../partials/head'); -%>\r\n    <style>\r\n        .img-product{\r\n            width: 100%;\r\n            height: 230px;\r\n            overflow: hidden;\r\n            text-align: center;\r\n        }\r\n        .img-product img{\r\n            width: 100%;\r\n            height: 100%;\r\n            object-fit: cover;\r\n        }\r\n        .img-detail{\r\n            width: 100%;\r\n            height: 370px;\r\n            overflow: hidden;\r\n            text-align: center;\r\n        }\r\n        .img-detail img{\r\n            width: 100%;\r\n            height: 100%;\r\n            object-fit: cover;\r\n        }\r\n        @media (min-width: 1025px) {\r\n            .h-custom {\r\n            height: 100vh !important;\r\n            }\r\n        }\r\n    </style>\r\n</head>\r\n\r\n<body>\r\n    <!-- Topbar Start -->\r\n    <%- include('../partials/header'); -%>\r\n    <!-- Topbar End -->\r\n    <!-- Navbar Start -->\r\n    <div class=\"container-fluid mb-5\">\r\n        <div class=\"row border-top px-xl-5\">\r\n            <% if (locals.dataLocal) { %>\r\n                <%- include('../partials/categorybar',{data : dataLocal}); -%>\r\n              <% }\r\n            %>\r\n            <div class=\"col-lg-9\">\r\n                <%- include('../partials/navbar'); -%>\r\n                <% if (page_layout == '/') { %>\r\n                    <%- include('../partials/slider'); -%>\r\n                  <% }\r\n                %>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <!-- Navbar End -->\r\n    <!-- Content -->\r\n    <% switch (page_layout) {\r\n        case '/' : %>\r\n            <%- include('./home',{data : products}); -%>\r\n            <% break;\r\n        case 'detail-product' : %>\r\n        <% if (dataDetail || comments) { %>\r\n        <%- include('./product-detail',{data : dataDetail,data1 : comments,data2:count}); -%>\r\n        <% } %>\r\n            <% break;\r\n        case 'products' : %>\r\n        <% if (danhmucsp && sanpham) { %>\r\n            <%- include('./products',{data1 : danhmucsp,data2 : sanpham}); -%>\r\n            <% } %>\r\n            <% break;\r\n        case 'search' : %>\r\n            <%- include('./search',{data : sanpham}); -%>\r\n            <% break;\r\n        case 'profile' : %>\r\n            <%- include('./profile',{data : user}); -%>\r\n            <% break;\r\n        case 'edit-user' : %>\r\n            <%- include('./editprofile',{data : user}); -%>\r\n            <% break;\r\n        case 'cart' : %>\r\n            <%- include('./cart',{data : products,data2 : Arr_giohang}); -%>\r\n            <% break;\r\n        case 'checkout' : %>\r\n            <%- include('./checkout',{data : products,data2 : Arr_giohang,data3 : user}); -%>\r\n            <% break;\r\n        case 'bills' : %>\r\n            <%- include('./bills'); -%>\r\n            <% break;\r\n        case 'contact' : %>\r\n            <%- include('./contact'); -%>\r\n            <% break;\r\n        default:\r\n            break;\r\n        } %>\r\n\r\n    <!-- Footer Start -->\r\n    <%- include('../partials/footer'); -%>\r\n    <!-- Footer End -->\r\n\r\n\r\n    <!-- Back to Top -->\r\n    <a href=\"#\" class=\"btn btn-primary back-to-top\"><i class=\"fa fa-angle-double-up\"></i></a>\r\n\r\n\r\n    <!-- JavaScript Libraries -->\r\n    <script src=\"https://code.jquery.com/jquery-3.4.1.min.js\"></script>\r\n    <script src=\"https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js\"></script>\r\n    <script src=\"/lib/easing/easing.min.js\"></script>\r\n    <script src=\"/lib/owlcarousel/owl.carousel.min.js\"></script>\r\n\r\n    <!-- Contact Javascript File -->\r\n    <script src=\"/mail/jqBootstrapValidation.min.js\"></script>\r\n    <script src=\"/mail/contact.js\"></script>\r\n\r\n    <!-- Template Javascript -->\r\n    <script src=\"/js/main.js\"></script>\r\n    <script>\r\n      const [...listStar] = document.querySelectorAll('.star-review');\r\n      const value_star = document.querySelector('#value_star');\r\n      //on click star setAtribute class fas fa-star for it and when two click remove class far fa-star for other star\r\n      let count = 0;  \r\n      //how to get count star fas fa-star\r\n        listStar.forEach(star => {\r\n            star.addEventListener('click', function() {\r\n            if (star.classList.contains('fas')) {\r\n                star.classList.remove('fas');\r\n                star.classList.add('far');\r\n                count--;\r\n            } else {\r\n                star.classList.remove('far');\r\n                star.classList.add('fas');\r\n                count++;\r\n            }\r\n            value_star.value = count;\r\n            });\r\n        });\r\n    </script>\r\n</body>\r\n\r\n</html>"
        }
    ]
